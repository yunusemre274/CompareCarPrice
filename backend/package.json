{
  "name": "global-car-compare-backend",
  "version": "1.0.0",
  "description": "Real-time car price comparison API with global economic data",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "prepare": "prisma generate",
    "build": "prisma generate && tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "lint": "tsc --noEmit",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "seed": "tsx src/scripts/seed.ts"
  },
  "keywords": [
    "car",
    "price",
    "comparison",
    "api",
    "real-time"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "axios": "^1.6.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "node-cache": "^5.1.2",
    "node-cron": "^4.2.1",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/helmet": "^0.0.48",
    "@types/node": "^24.10.1",
    "@types/node-cron": "^3.0.11",
    "nodemon": "^3.0.2",
    "pino-pretty": "^13.1.2",
    "prisma": "^6.19.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
